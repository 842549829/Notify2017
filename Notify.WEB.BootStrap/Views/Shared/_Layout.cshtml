@RenderBody()

<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="renderer" content="webkit" />
    <meta http-equiv="Cache-Control" content="no-siteapp" />
    <title> 主页</title>

    <!--[if lt IE 9]>
    <meta http-equiv="refresh" content="0;ie.html" />
    <![endif]-->

    <link href="~/Content/Scripts/tools/hplus/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/Scripts/tools/hplus/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/Content/Scripts/tools/hplus/css/animate.min.css" rel="stylesheet">
    <link href="~/Content/Scripts/tools/hplus/css/style.min.css" rel="stylesheet">
</head>

<body class="fixed-sidebar full-height-layout gray-bg" style="overflow:hidden">
    <div id="wrapper">
        <!--左侧导航开始-->
        <nav class="navbar-default navbar-static-side" role="navigation">
            <div class="nav-close">
                <i class="fa fa-times-circle"></i>
            </div>
            <div class="sidebar-collapse">
                @{Html.RenderAction("QueryTMenu", "Home");}
            </div>
        </nav>
        <!--左侧导航结束-->
        <!--右侧部分开始-->
        <div id="page-wrapper" class="gray-bg dashbard-1">
            <div class="row border-bottom">
                <nav class="navbar navbar-static-top" role="navigation" style="margin-bottom: 0">
                    <div class="navbar-header">
                        <a class="navbar-minimalize minimalize-styl-2 btn btn-primary " href="#"><i class="fa fa-bars"></i> Notify</a>
                    </div>
                </nav>
            </div>
            <div class="row content-tabs">
                <button class="roll-nav roll-left J_tabLeft">
                    <i class="fa fa-backward"></i>
                </button>
                <nav class="page-tabs J_menuTabs">
                    <div class="page-tabs-content">
                        <a href="javascript:;" class="active J_menuTab" data-id="/home/index">首页</a>
                    </div>
                </nav>
                <button class="roll-nav roll-right J_tabRight">
                    <i class="fa fa-forward"></i>
                </button>
                <div class="btn-group roll-nav roll-right">
                    <button class="dropdown J_tabClose" data-toggle="dropdown">
                        关闭操作<span class="caret"></span>

                    </button>
                    <ul role="menu" class="dropdown-menu dropdown-menu-right">
                        <li class="J_tabShowActive">
                            <a>定位当前选项卡</a>
                        </li>
                        <li class="divider"></li>
                        <li class="J_tabCloseAll">
                            <a>关闭全部选项卡</a>
                        </li>
                        <li class="J_tabCloseOther">
                            <a>关闭其他选项卡</a>
                        </li>
                    </ul>
                </div>
                <a href="#" class="roll-nav roll-right J_tabExit"><i class="fa fa fa-sign-out"></i> 退出</a>
            </div>
            <div class="row J_mainContent" id="content-main">
                <iframe class="J_iframe" name="iframe0" style="width:100%;height:100%; border:0;" src="@Url.Action("Default", "Home")" data-id="/home/default" seamless></iframe>
            </div>
            <div class="footer">
                <div class="pull-right">
                    &copy; 2016-2018 <a href="#" target="_blank">notify</a>
                </div>
            </div>
        </div>
    </div>
    <script src="~/Content/Scripts/tools/hplus/js/jquery.min.js"></script>
    <script src="~/Content/Scripts/tools/hplus/js/bootstrap.min.js"></script>
    <script src="~/Content/Scripts/tools/hplus/js/plugins/metisMenu/jquery.metisMenu.js"></script>
    <script src="~/Content/Scripts/tools/hplus/js/plugins/slimscroll/jquery.slimscroll.min.js"></script>
    <script src="~/Content/Scripts/tools/hplus/js/plugins/layer/layer.min.js"></script>
    <script src="~/Content/Scripts/tools/hplus/js/hplus.min.js"></script>
    <script src="~/Content/Scripts/tools/hplus/js/contabs.min.js" type="text/javascript"></script>
    <script src="~/Content/Scripts/tools/hplus/js/plugins/pace/pace.min.js"></script>
</body>
</html>